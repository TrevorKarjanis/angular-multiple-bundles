{
  "name": "angular-multi-element",
  "version": "0.0.0",
  "scripts": {
    "app-element:build:prod": "ng build --configuration production app-element",
    "app-element:xi18n": "locl extract -s=\"dist/app-element/*.js\" -f=json -o=\"projects/app-element/src/assets/locales/en.json\"",
    "app-element-2:build:prod": "ng build --configuration production app-element-2",
    "app-element-2:xi18n": "locl extract -s=\"dist/app-element-2/*.js\" -f=json -o=\"projects/app-element-2/src/assets/locales/en.json\"",
    "build": "npm run app-element:build:prod && npm run app-element-2:build:prod",
    "lint": "ng lint",
    "ng": "ng",
    "start": "http-server ./dist",
    "xi18n": "npm run app-element:xi18n && npm run app-element-2:xi18n"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.0",
    "@angular/cdk": "^11.0.0",
    "@angular/common": "~13.1.0",
    "@angular/compiler": "~13.1.0",
    "@angular/core": "~13.1.0",
    "@angular/elements": "^13.1.0",
    "@angular/forms": "~13.1.0",
    "@angular/localize": "^13.1.0",
    "@angular/material": "^11.0.0",
    "@angular/platform-browser": "~13.1.0",
    "@angular/platform-browser-dynamic": "~13.1.0",
    "@angular/router": "~13.1.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^13.0.0",
    "@angular-devkit/build-angular": "~13.1.1",
    "@angular/cli": "~13.1.1",
    "@angular/compiler-cli": "~13.1.0",
    "@locl/cli": "^1.0.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "http-server": "^0.12.3",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "^4.5.3"
  }
}
